<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Intake_in_Future</fullName>
    <active>true</active>
    <description>Prevent intakes in the future</description>
    <errorConditionFormula>yintake_v2__c &gt; TODAY()</errorConditionFormula>
    <errorDisplayField>yintake_v2__c</errorDisplayField>
    <errorMessage>You can&apos;t intake a patient in the future. Please specify a valid Date of Patient Intake.</errorMessage>
</ValidationRule>
